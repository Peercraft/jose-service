{
    "name": "spomky-labs/jose-service",
    "description": "JSON Object Signing and Encryption service for PHP.",
    "type": "project",
    "license": "MIT",
    "keywords": ["JWS", "JWT", "JWE", "JWA", "JWK", "JWKSet", "Jose"],
    "homepage": "https://github.com/Spomky-Labs/jose-service",
    "authors": [
        {
            "name": "Florent Morselli",
            "homepage": "https://github.com/Spomky-Labs/jose-service/contributors"
        }
    ],
    "autoload": {
        "psr-4": {
            "SpomkyLabs\\Service\\":  "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SpomkyLabs\\Service\\Tests\\":  "tests/"
        }
    },
    "require": {
        "php": ">=5.3",
        "spomky-labs/jose": "~0.0.9",
        "pimple/pimple": "~3.0.0"
    },
    "require-dev": {
        "phpseclib/phpseclib": "~2.0.0",
        "mdanter/ecc": "~0.3",
        "phpunit/phpunit": "~4.5",
        "lib-openssl": "*",
        "ext-mcrypt": "*",
        "spomky-labs/pbkdf2": "~1.0",
        "spomky-labs/aes-key-wrap": "~2.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.0.x-dev"
        }
    }
}
